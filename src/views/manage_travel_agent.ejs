        <% for(i=0;i<data.length;i++) {%>
          <tr>
            <td>
              <%= i+1%>
            </td>
            <td>
              <%= data[i].name%>
            </td>

            <td>
              <%= data[i].email%>
            </td>
            <%if(data[i].status==="0"){%>
              <td><a href="/admin/approve-agent/<%= data[i]._id%>"><button class="btn btn-info"
                    type="button">Approve</button></a>
                <a href="/admin/delete-agent/<%= data[i]._id%>"><button class="btn btn-danger"
                    type="button">Delete</button></a>
              </td>
              <%}else{%>
                <td><button class="btn btn-info" type="button">Approved</button><a
                    href="/admin/delete-agent/<%= data[i]._id%>">
                    <button class="btn btn-danger" type="button">Delete</button>
                  </a></td>
                <%}%>
          </tr>
          <%}%>
            
